{   
    // Required
    "name": "FullStack Backgrounds",
    "version": "1.0",
    "manifest_version": 2,

    // Recommended
    "description": "Because FullStack should be everywhere",
    "icons": {
        "128":"ironman.png"
    },

    // Type of action: Browser, Page or none
    "browser_action": {
        "default_title": "Always have the fullstack team with you...",
        "default_popup": "popup.html"
    },

    // intent of app, what you will access from the user
    "permissions": ["http://*/*", "https://*/*"]
}